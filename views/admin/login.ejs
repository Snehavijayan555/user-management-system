<%- include('../layouts/header.ejs') -%>

<h3 class="fw-bold text-center mt-2">Admin Login</h3>

<% if(typeof message !== 'undefined'){ %>
<h3><%= message %></h3>
<% } %>
<div class="text-center">
  <center>
  <form action="" method="POST" class="text-center col-6">
  <input type="email" name="email" class="form-control mt-4" placeholder="Enter Email" />
  <input type="password" name="password" class="form-control mt-3" placeholder="Enter password" />
  <div class="text-end">
    <input type="submit" class="btn btn-info mt-3 col-3" value="Login" />
  </div>
</center>
</form>
</div>

<%- include('../layouts/footer.ejs') -%>
